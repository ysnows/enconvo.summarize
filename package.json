{
  "name": "enconvo.summarize",
  "version": "1.0.0",
  "type": "module",
  "description": "",
  "main": "index.js",
  "scripts": {
    "tsup": "tsup",
    "start": "tsx -r dotenv/config src/index.ts",
    "lint": "eslint src",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --write \"**/*.ts\"",
    "format:check": "prettier --list-different \"**/*.ts\"",
    "build": "webpack",
    "deploy": "webpack &&  cp -r dist/* ~/Library/Application\\ Support/Enconvo/extension/enconvo.summarize",
    "publish": "webpack && mv dist enconvo.summarize && zip -r enconvo.summarize.zip enconvo.summarize && rm -rf enconvo.summarize",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ysnows/enconvo.summarize.git"
  },
  "author": "ysnows",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ysnows/enconvo.summarize/issues"
  },
  "homepage": "https://github.com/ysnows/enconvo.summarize#readme",
  "sideEffects": [
    "*.js"
  ],
  "dependencies": {
    "@dqbd/tiktoken": "^1.0.7",
    "@mozilla/readability": "^0.4.4",
    "axios": "^1.4.0",
    "dotenv": "^16.3.1",
    "jsdom": "^22.1.0",
    "jsonrpc-lite": "^2.2.0",
    "langchain": "^0.0.104",
    "node-readability": "^3.0.0",
    "openai": "^3.2.1",
    "process": "^0.11.10",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/js-yaml": "^4",
    "@types/node": "^18.15.5",
    "@typescript-eslint/eslint-plugin": "^5.51.0",
    "@typescript-eslint/parser": "^5.51.0",
    "copy-webpack-plugin": "^11.0.0",
    "eslint": "^8.33.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-prettier": "^4.2.1",
    "ignore-webpack-plugin": "^0.3.0",
    "prettier": "^2.8.3",
    "tsup": "^7.1.0",
    "tsx": "^3.12.3",
    "typescript": "^5.0.4",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4"
  }
}
